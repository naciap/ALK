#include <iostream>
#include <stdio.h>
#include <stdlib.h>
using namespace std;


// Napisz program wyznaczający podzbiór T o randze r w uporządkowaniu leksykograficznym
// k-elementowych podzbiorów zbioru {1, . . . , n}.

int main() {
  int Newton[20][20], q=19;
	for(int i=0;i<q;i++){
  		for(int j=0;j<=i;j++){
  			if(i==j || j==0 ) Newton[i][j]=1;
  			else Newton[i][j] = Newton[i-1][j-1] + Newton[i-1][j];
  		}
	}
	
	int n, k, r;
	
	scanf("%d %d %d", &n, &k, &r);
	
	int *T = new int[k+2];

	int x=1;
	for(int i=1; i<=k; i++){
		while(Newton[n-x][k-i]<=r){
			r-=Newton[n-x][k-i];
			x++;
		} 
		T[i]=x;
		x++;
	}
	
	for(int i=1;i<=k;i++){
		printf("%d ", T[i]);
	}
	
	return 0;
}
